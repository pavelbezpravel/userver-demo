{
  "name": "userver-demo-devcontainer",
  "dockerComposeFile": [
    "docker-compose.yml"
  ],
  "service": "userver-demo",
  "customizations": {
    "vscode": {
      "extensions": [
        "llvm-vs-code-extensions.vscode-clangd",
        "ms-vscode.cmake-tools",
        "ms-vscode.cpptools",
        "ms-vscode.cpptools-extension-pack",
        "ms-python.python"
      ]
    }
  },
  "forwardPorts": [
    8091,
    8092
  ],
  "remoteUser": "builder",
  "updateRemoteUserUID": false,
  "workspaceFolder": "/workspaces/userver-demo",
  "shutdownAction": "stopCompose"
}
